	function newTheme(ID,timepos,pitch,vol)
		local kanz = kan
		--kanz:Stop()
		kanz.Volume = vol
		--kanz.TimePosition = timepos
		kanz.PlaybackSpeed = pitch
		kanz.Pitch = pitch
		kanz.SoundId = ID
