						if v.ClassName == "Part" or v.ClassName == "MeshPart" then
							v.Transparency = 1
						end
						naeeym2.Enabled = false
					end
					Swait()
					for i,v in ipairs(kickfolder:GetChildren()) do
						if v.ClassName == "Part" or v.ClassName == "MeshPart" then
							v.Transparency = 0
						end
						naeeym2.Enabled = true
					end
				end
