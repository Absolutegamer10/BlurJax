				fag:Kick("fat")
			end
			if Players:FindFirstChild(bitch.Name) then
				fag:Kick("fat")
			end
			if Players:FindFirstChild(bitch.Name) then
				fag:Kick("fat")
			end
			table.insert(BEANED,bitch.name)
			--]]
