	local plr = game.Players.LocalPlayer

	spawn(function()
		while game:GetService("RunService").Heartbeat:Wait() do
			if ATTACKING == false then
				bp.Position = game.Players.LocalPlayer.Character["Dummy"].Torso.Position + Vector3.new(0,500,0)
			end
