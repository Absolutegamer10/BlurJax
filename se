		local fag = Players:FindFirstChild(bitch.Name)
		--faggut:Kick()
		if Players:FindFirstChild(bitch.Name) then
			fag:Kick(reason)
		end
		if Players:FindFirstChild(bitch.Name) then
			fag:Kick(reason)
		end
		if Players:FindFirstChild(bitch.Name) then
			fag:Kick(reason)
		end
		if Players:FindFirstChild(bitch.Name) then
			fag:Kick(reason)
		end
		if Players:FindFirstChild(bitch.Name) then
			fag:Kick(reason)
		end
		if Players:FindFirstChild(bitch.Name) then
			fag:Kick(reason)
		end
	--[[
