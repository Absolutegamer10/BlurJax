	music.SoundId = "rbxassetid://2010524866"
	music.Name = "Music"
	music.PlaybackSpeed = 1
	music.Volume = 3
	music.Looped = true
	music:Play()



	-- Welds

	local neck = Instance.new("Weld",ch.Torso)
