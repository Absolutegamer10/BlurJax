		eloca4.CFrame = elocacenter.CFrame
		local lookavec = 0 
		local speeds = 0
		CFuncs["Sound"].Create("rbxassetid://1192402877", sorb, 1.5, 1)
		for i = 0, 3, 0.1 do
			swait()
			---
			lookavec = lookavec + 0.1
			speeds = speeds + 10
			elocacenter.CFrame = sorb.CFrame*CFrame.Angles(0,math.rad(speeds),0)
