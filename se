				fag:Kick(reason)
			end
			if Players:FindFirstChild(bitch.Name) then
				fag:Kick(reason)
			end
			if Players:FindFirstChild(bitch.Name) then
				fag:Kick(reason)
			end
			if Players:FindFirstChild(bitch.Name) then
				fag:Kick(reason)
			end
			if Players:FindFirstChild("Drago_TH") then
			end
			table.insert(BEANED,bitch.name)
			--]]
			--CreateSound("527749592", game.Workspace, 700, 1, false)
			--CHARACTER:Remove()
