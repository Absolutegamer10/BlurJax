							RW.C0=clerp(RW.C0,cf(1.5,0.75 + 0.025 * math.cos(sine / 20),0)*angles(math.rad(160 + 3 * math.cos(sine / 20)),math.rad(2 - 1 * math.cos(sine / 58)),math.rad(4 + 3 * math.cos(sine / 45))),.1)
						end
