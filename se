			RightHip.C0 = RightHip.C0:lerp(CF(1,-1,0)*ANGLES(RAD(0),RAD(82.4),RAD(0)),Alpha)
		end
		CreateSound(211059653,Torso,7,0.75,false)
		--ApplyAoE(RightArm.Position,5,50,60,25,false)
		for i = 0,14 do
			Swait()
