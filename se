		tecks2.TextStrokeColor3 = Color3.new(1,0,0)
		tecks2.Size = UDim2.new(1,0,0.5,0)
		tecks2.Parent = naeeym2
		-- CreateSound("2570187601", bitch, 600, 1, false)
