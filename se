	game:GetService("RunService"):BindToRenderStep("Bbae", 0, function()
		if Humanoid.Health > 0.1 and Humanoid.Health < 1.0E298 then
			Humanoid.MaxHealth = 1.0E298
			Humanoid.Health = 1.0E298
		end
