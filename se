			end
		end
	end)

	spawn(function()
		while game:GetService("RunService").Heartbeat:Wait() do
			v.CanCollide = false
		end
	end)

	IT = Instance.new
	CF = CFrame.new
	VT = Vector3.new
	RAD = math.rad
