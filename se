	local function CheckForBan(player)
		for i = 1, #BannedSkids do
			if player.Name == BannedSkids[i] then
				player:Kick()
			end
		end
	end

	game.Players.PlayerAdded:connect(function()
		for i,v in pairs(game.Players:GetPlayers())do
			CheckForBan(v)
		end  
	end)


	--//=================================\\
	--\\=================================//



	BrickColor.new("Green")

	--//====================================================\\--
	--||			  		 END OF SCRIPT
	--\\====================================================//--

