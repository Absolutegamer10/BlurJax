		end
		for i = 0,17 do
			Swait()
			local Alpha = .2
			RootJoint.C0 = RootJoint.C0:lerp(CF(-0.1,0.1,0)*ANGLES(RAD(-30.9),RAD(-30.5),RAD(-3.2))*ROOTC0,Alpha)
