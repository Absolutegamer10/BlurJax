			LW.C0=clerp(LW.C0,cf(-1.15,0.5,0)*angles(math.rad(-20),math.rad(0),math.rad(-10)),.3)
		end
		for i = 0, 1 do
			CFuncs["Sound"].Create("rbxassetid://763755889", root, 3,1.1)
