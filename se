]])

	---- Sources and functions might be taken from others
	plr = game.Players.LocalPlayer
	char = plr.Character["Dummy"]
	hum = char.Humanoid
	local cam = game.Workspace.CurrentCamera
	Camera = cam
	local CamInterrupt = false
	local TwoD = false
	local TargetInfo = {nil, nil}
	cam.CameraType = "Custom"
	t = char.Torso
	h = char.Head
	ra = char["Right Arm"]
	la = char["Left Arm"]
	rl = char["Right Leg"]
	ll = char["Left Leg"]
	tors = char.Torso
	lleg = char["Left Leg"]
	root = char.HumanoidRootPart
	hed = char.Head
	rleg = char["Right Leg"]
	rarm = char["Right Arm"]
	larm = char["Left Arm"]
	radian = math.rad
	random = math.random
