			local rtype = rottype
			for i=0,1,delay do
				swait()
				if rtype == 1 then
					prt.CFrame = prt.CFrame*CFrame.Angles(0,0,0.1)
				elseif rtype == 2 then
					prt.CFrame = prt.CFrame*CFrame.Angles(0,0,-0.1)
				end
				prt.Transparency=i
				Mesh.Scale=Mesh.Scale+vt(x3,y3,0)
			end
			Part.Parent=nil
		end),prt,msh)
	end

	function MagicShockAltCircle(brickcolor,cframe,x1,z1,x3,z3,delay,rottype)
